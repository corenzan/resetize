{
  "name": "resetize",
  "version": "26.0.0",
  "description": "CSS normalization with opinionated resets.",
  "main": "dist/resetize.css",
  "style": "dist/resetize.css",
  "files": [
    "README.md",
    "LICENSE.md",
    "dist/resetize.css"
  ],
  "scripts": {
    "test": "echo $npm_package_version",
    "build": "postcss -m -o dist/resetize.css src/*.css",
    "watch": "npm run build -- -w",
    "version": "npm run build && sed -i \"\" -e \"s/\\$version/$npm_package_version/\" dist/resetize.css && git add dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corenzan/resetize.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corenzan/resetize/issues"
  },
  "homepage": "https://github.com/corenzan/resetize#readme",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "postcss-cli": "^6.1.2"
  }
}
