{
  "name": "resetize",
  "version": "29.0.0",
  "description": "CSS normalization with opinionated resets.",
  "style": "dist/resetize.css",
  "main": "dist/resetize.css",
  "targets": {
    "main": false
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "dist/resetize.css"
