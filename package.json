{
  "name": "resetize",
  "version": "24.5.0",
  "description": "An opinionated CSS reset",
  "main": "dist/resetize.css",
  "style": "dist/resetize.css",
  "files": [
    "LICENSE.md",
    "resetize.css"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "postcss -o dist/resetize.css src/*.css",
    "preversion": "sed -i -e \"s/$npm_package_version/%version%/\" src/resetize.css",
    "version": "sed -i -e \"s/%version%/$npm_package_version/\" src/resetize.css && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corenzan/resetize.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corenzan/resetize/issues"
  },
  "homepage": "https://github.com/corenzan/resetize#readme",
  "devDependencies": {
    "autoprefixer": "^9.4.3",
    "cssnano": "^4.1.7",
    "cssnano-preset-advanced": "^4.0.5",
    "postcss-cli": "^6.1.0"
  }
}
